#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/taro/Desktop/projects/ruoyi-vue-pro-master-jdk17-202601201221/yudao-ui/yudao-ui-admin-vue3/node_modules/.pnpm/jsonrepair@3.13.0/node_modules/jsonrepair/bin/node_modules:/Users/taro/Desktop/projects/ruoyi-vue-pro-master-jdk17-202601201221/yudao-ui/yudao-ui-admin-vue3/node_modules/.pnpm/jsonrepair@3.13.0/node_modules/jsonrepair/node_modules:/Users/taro/Desktop/projects/ruoyi-vue-pro-master-jdk17-202601201221/yudao-ui/yudao-ui-admin-vue3/node_modules/.pnpm/jsonrepair@3.13.0/node_modules:/Users/taro/Desktop/projects/ruoyi-vue-pro-master-jdk17-202601201221/yudao-ui/yudao-ui-admin-vue3/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/taro/Desktop/projects/ruoyi-vue-pro-master-jdk17-202601201221/yudao-ui/yudao-ui-admin-vue3/node_modules/.pnpm/jsonrepair@3.13.0/node_modules/jsonrepair/bin/node_modules:/Users/taro/Desktop/projects/ruoyi-vue-pro-master-jdk17-202601201221/yudao-ui/yudao-ui-admin-vue3/node_modules/.pnpm/jsonrepair@3.13.0/node_modules/jsonrepair/node_modules:/Users/taro/Desktop/projects/ruoyi-vue-pro-master-jdk17-202601201221/yudao-ui/yudao-ui-admin-vue3/node_modules/.pnpm/jsonrepair@3.13.0/node_modules:/Users/taro/Desktop/projects/ruoyi-vue-pro-master-jdk17-202601201221/yudao-ui/yudao-ui-admin-vue3/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jsonrepair@3.13.0/node_modules/jsonrepair/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../jsonrepair@3.13.0/node_modules/jsonrepair/bin/cli.js" "$@"
fi
